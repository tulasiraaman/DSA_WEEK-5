class Solution {
    public int findMin(int[] nums) {
        int left = 0, right = nums.length-1;
        
        while(left < right){
            int mid = left + (right - left)/2;

            if(nums[mid] < nums[right]){
                right = mid;
            }else{
                left = mid+1;
            }
        }
        return nums[left];
    }
    //compare mid with right, which decide which half has the pivot.
    //Shrink search space until only one element remains.
    //TC - O(log n)
}
