class Solution {
    public int findFloor(int[] arr, int x) {
        // code here
        if (arr == null || arr.length == 0) return -1;

        int left = 0, right = arr.length-1;
        int ans = -1;

        while(left <= right){
            int mid = left + (right - left)/2;
            if(arr[mid] <= x){
                ans = mid;
                left = mid+1;
            }else{
                right = mid-1;
            }
        }
        return ans; 
    }
}
